
<h1>Listing teens</h1>
 
<%= link_to 'Signup with us!', new_teen_path %>
<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
  </tr>
 
  <% @teens.each do |teen| %>
    <tr>
      <td><%= teen.fname %></td>
      <td><%= teen.lname %></td>
      <td><%= link_to 'Show', teen_path(teen) %></td>
    </tr>
  <% end %>
</table>





